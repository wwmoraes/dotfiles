[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.applescript]
indent_style = tab
indent_size = 2

[*.html]
insert_final_newline = false

[*.json]
insert_final_newline = false

[*.md]
trim_trailing_whitespace = false

[.vscode/settings.json]
insert_final_newline = false

[vscode/.config/Code/User/settings.json]
insert_final_newline = false

[Makefile]
indent_style = tab
