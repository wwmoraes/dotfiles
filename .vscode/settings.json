{
  "Lua.completion.autoRequire": false,
  "Lua.diagnostics.globals": [
    "hs",
    "spoon"
  ],
  "Lua.workspace.ignoreDir": [
    ".vscode",
    "~/.hammerspoon"
  ],
  "Lua.workspace.library": [
    "~/.shadow.d/.hammerspoon/Spoons",
    "~/dev/spoons"
  ],
  "files.associations": {
    "**/*dot_ssh/*config*": "ssh_config",
    "**/*dot_ssh/*config.d/*/*": "ssh_config",
    "**/*environment.d/*.conf*": "dotenv",
    "**/environment.rm.conf*": "properties",
    "**/pip.conf*": "ini",
    "*.conf.tmpl": "properties",
    "*.fish.tmpl": "fish",
    "*.json.tmpl": "jsonc",
    "*.nix.tmpl": "nix",
    "*.plist": "xml",
    "*.plist.tmpl": "xml",
    "*.sh.tmpl": "shellscript",
    "*.toml.tmpl": "toml",
    "*.yaml.tmpl": "yaml",
    "*dot_Brewfile*": "ruby",
    "*dot_bash*": "shellscript",
    "*dot_editorconfig": "editorconfig",
    "*dot_env*": "dotenv",
    "*dot_gitaliases*": "gitconfig",
    "*dot_gitcolors*": "gitconfig",
    "*dot_gitconfig*": "gitconfig",
    "*dot_gitcorlors*": "gitconfig",
    "*dot_grc/conf.*": "grc-app",
    ".chezmoiignore": "gitignore",
    ".chezmoiremove": "gitignore"
  },
  "files.exclude": {
    "**/Code/User/History/**/*": true,
    ".direnv": true,
    ".task/checksum": true,
    "LICENSE": true,
    "SECURITY.md": true,
    "CONTRIBUTING.md": true,
    "CODE_OF_CONDUCT.md": true
  },
  "go-template.languages": [
    "ini",
    "toml",
    "yaml",
    "xml",
    "fish",
    "json",
    "ruby",
    "gitconfig",
    "properties",
    "plaintext",
    "dotenv",
    "ssh_config",
    "shellscript"
  ],
  "go-template.patterns": [
    "**/*.tmpl"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "*dot_docker/*daemon.json*"
      ],
      "url": "https://json.schemastore.org/dockerd.json"
    }
  ],
  "nixEnvSelector.nixFile": "${workspaceFolder}/shell.nix"
}
