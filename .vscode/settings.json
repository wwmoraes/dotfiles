{
  "Lua.completion.autoRequire": false,
  "Lua.diagnostics.globals": [
    "hs",
    "spoon"
  ],
  "Lua.workspace.ignoreDir": [
    ".vscode",
    "~/.hammerspoon"
  ],
  "Lua.workspace.library": [
    "~/Applications/Hammerspoon.app/Contents/Resources/extensions",
    "~/.files/.systems/darwin/hammerspoon/.hammerspoon/Spoons",
    "~/dev/spoons"
  ],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".setup.d/packages/**/*.txt"
  ],
  "cSpell.words": [
    "applescript",
    "autofetch",
    "Browserosaurus",
    "caffeinate",
    "dont",
    "eventtap",
    "fblid",
    "fileutils",
    "Fira",
    "fluidapp",
    "gclid",
    "gitlens",
    "helmignore",
    "kdestroy",
    "keepcoder",
    "kinit",
    "kubeconfig",
    "Kubeconfigs",
    "mkdir",
    "ONCALL",
    "osascript",
    "Pylance",
    "slackmacgap",
    "terraformignore",
    "thuisbezorgd",
    "tinc",
    "tinyspeck",
    "tldrnewsletter",
    "urlevent",
    "vsicons"
  ],
  "files.associations": {
    "**/.gitaliases": "gitconfig",
    "**/.gitcolors": "gitconfig",
    "**/.gitignore*": "gitignore",
    "grc/.grc/conf.*": "grc-app"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true
  },
  "workspace.isHidden": true
}