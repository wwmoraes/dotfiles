{
  "files.associations": {
    "**/.gitaliases": "gitconfig",
    "grc/.grc/conf.*": "grc-app"
  },
  "cSpell.words": [
    "Fira",
    "Kubeconfigs",
    "Pylance",
    "autofetch",
    "dont",
    "fileutils",
    "gitlens",
    "helmignore",
    "kubeconfig",
    "terraformignore",
    "vsicons"
  ],
  "Lua.diagnostics.globals": ["hs", "spoon"],
  "Lua.workspace.library": {
    "/Applications/Hammerspoon.app/Contents/Resources/extensions/": true,
    "~/.hammerspoon/Spoons/": true
  }
}
