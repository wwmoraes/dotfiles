{
  "Lua.completion.autoRequire": false,
  "Lua.diagnostics.globals": [
    "hs",
    "spoon"
  ],
  "Lua.workspace.ignoreDir": [
    ".vscode",
    "~/.hammerspoon"
  ],
  "Lua.workspace.library": [
    "~/.shadow.d/.hammerspoon/Spoons",
    "~/dev/spoons"
  ],
  "cSpell.ignorePaths": [
    "**/.git/objects/**",
    ".vscode",
    ".setup.d/packages/**/*.txt"
  ],
  "cSpell.words": [
    "chezmoi",
    "endregion"
  ],
  "files.associations": {
    "**/*dot_ssh/*config*": "ssh_config",
    "**/*dot_ssh/*config.d/*/*": "ssh_config",
    "**/*environment.d/*.conf*": "dotenv",
    "**/*paths.d/*.conf*": "properties",
    "**/environment.rm.conf*": "properties",
    "**/pip.conf*": "ini",
    "*.conf.tmpl": "properties",
    "*.fish.tmpl": "fish",
    "*.plist": "xml",
    "*.plist.tmpl": "xml",
    "*.sh.tmpl": "shellscript",
    "*.toml.tmpl": "toml",
    "*.yaml.tmpl": "yaml",
    "*dot_Brewfile*": "ruby",
    "*dot_bash*": "shellscript",
    "*dot_editorconfig": "editorconfig",
    "*dot_env*": "dotenv",
    "*dot_gitaliases*": "gitconfig",
    "*dot_gitconfig*": "gitconfig",
    "*dot_gitcorlors*": "gitconfig",
    "*dot_grc/conf.*": "grc-app",
    ".chezmoiignore": "gitignore",
    ".chezmoiremove": "gitignore"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true
  },
  "go-template.languages": [
    "ini",
    "toml",
    "yaml",
    "xml",
    "fish",
    "ruby",
    "gitconfig",
    "properties",
    "plaintext",
    "dotenv",
    "ssh_config",
    "shellscript"
  ],
  "xml.format.enabled": false
}