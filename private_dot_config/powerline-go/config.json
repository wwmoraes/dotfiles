{
  "hostname-only-if-ssh": true,
  "modules": [
    "user",
    "host",
    "ssh",
    "cwd",
    "docker",
    "kube",
    "git"
  ],
  "modules-right": [
    "newline",
    "nix-shell",
    "perms",
    "jobs",
    "exit",
    "root"
  ],
  "numeric-exit-codes": true,
  "path-aliases": {
    "~": "🏡",
    "~/.local/share/chezmoi": "🛠️",
    "~/dev": "💻",
    "~/workspace": "🏢"
  },
  "shell": "bare"
}