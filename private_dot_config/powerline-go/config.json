{
  "cwd-max-depth": 3,
  "duration-low-precision": true,
  "duration-min": "0.6",
  "hostname-only-if-ssh": true,
  "modules": [
    "user",
    "host",
    "ssh",
    "cwd",
    "docker",
    "kube",
    "git"
  ],
  "modules-right": [
    "newline",
    "duration",
    "nix-shell",
    "perms",
    "jobs",
    "exit",
    "root"
  ],
  "priority": [
    "newline",
    "jobs",
    "exit",
    "user",
    "host",
    "ssh",
    "cwd",
    "root",
    "nix-shell",
    "dotenv",
    "goenv",
    "perms",
    "git",
    "duration",
    "docker",
    "kube"
  ],
  "numeric-exit-codes": true,
  "path-aliases": {
    "~": "🏡",
    "~/.local/share/chezmoi": "🧰",
    "~/dev": "💻",
    "~/workspace": "🏢"
  },
  "shell": "bare"
}
