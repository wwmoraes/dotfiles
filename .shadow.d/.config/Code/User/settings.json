  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.cursorSurroundingLines": 5,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": "'ss01', 'ss03', 'ss05', 'ss07', 'ss09', 'cv12', 'cv21', 'cv25', 'cv26', 'cv28', 'cv29', 'cv30'",
  "editor.fontSize": 16,
  "editor.fontWeight": "400",
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": "inline",
    "strings": "inline"
  },
  "editor.renderFinalNewline": "off",
  "editor.rulers": [
    80,
    100
  ],
  "editor.screenReaderAnnounceInlineSuggestion": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "explorer.confirmDelete": true,
  "files.associations": {
    "**/.ssh/config": "ssh_config",
    "**/.ssh/config.d/*/*": "ssh_config",
    "**/i3/config": "i3",
    "*.azure-pipelines.yaml": "azure-pipelines",
    "*.azure-pipelines.yml": "azure-pipelines",
    "*.gotmpl": "go-template",
    "*.gtpl": "go-template",
    "*.hsfiles": "mustache",
    "*.tfstate": "json",
    "*.tpl": "go-template",
    "*.yml": "yaml",
    ".coveragerc": "ini",
    ".env*": "dotenv",
    ".gitaliases": "properties",
    ".gitconfig": "properties",
    ".gitignore-global": "ignore",
    ".helmignore": "gitignore",
    ".style.yapf": "ini",
    ".terraformignore": "gitignore",
    "azure-pipelines.yaml": "azure-pipelines",
    "azure-pipelines.yml": "azure-pipelines"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true
  },
  "files.trimFinalNewlines": true,
  "git-graph.dialog.merge.noFastForward": false,
  "git-graph.dialog.merge.squashCommits": true,
  "git-graph.maxDepthOfRepoSearch": 2,
  "git.autoStash": true,
  "git.autofetch": true,
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.statusBar.enabled": false,
  "go.coverOnSave": false,
  "go.delveConfig": {
    "debugAdapter": "legacy"
  },
  "go.lintOnSave": "package",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "go.vetOnSave": "package",
  "gremlins.characters": {
    "2013": {
      "description": "en dash",
      "level": "warning"
    },
    "2018": {
      "description": "left single quotation mark",
      "level": "warning"
    },
    "2019": {
      "description": "right single quotation mark",
      "level": "warning"
    },
    "2029": {
      "description": "paragraph separator",
      "level": "error",
      "zeroWidth": true
    },
    "0003": {
      "description": "end of text",
      "level": "warning"
    },
    "000b": {
      "description": "line tabulation",
      "level": "warning"
    },
    "00a0": {
      "description": "non breaking space",
      "level": "info"
    },
    "00ad": {
      "description": "soft hyphen",
      "level": "info"
    },
    "200b": {
      "description": "zero width space",
      "level": "error",
      "zeroWidth": true
    },
    "200c": {
      "description": "zero width non-joiner",
      "level": "warning",
      "zeroWidth": true
    },
    "200e": {
      "description": "left-to-right mark",
      "level": "error",
      "zeroWidth": true
    },
    "201c": {
      "description": "left double quotation mark",
      "level": "warning"
    },
    "201d": {
      "description": "right double quotation mark",
      "level": "warning"
    },
    "202c": {
      "description": "pop directional formatting",
      "level": "error",
      "zeroWidth": true
    },
    "202d": {
      "description": "left-to-right override",
      "level": "error",
      "zeroWidth": true
    },
    "202e": {
      "description": "right-to-left override",
      "level": "error",
      "zeroWidth": true
    },
    "fffc": {
      "description": "object replacement character",
      "level": "error",
      "zeroWidth": true
    }
  },
  "haskell.formattingProvider": "stylish-haskell",
  "haskell.manageHLS": "GHCup",
  "haskell.plugin.cabal.globalOn": false,
  "haskell.toolchain": {
    "cabal": null,
    "ghc": null,
    "stack": null
  },
  "haskell.upgradeGHCup": true,
  "html.format.indentInnerHtml": true,
  "http.proxySupport": "fallback",
  "javascript.format.semicolons": "insert",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.format.enable": true,
  "json.schemaDownload.enable": true,
  "json.schemas": [
    {
      "fileMatch": [
        "/package.json"
      ],
      "url": "https://json.schemastore.org/package.json"
    },
    {
      "fileMatch": [
        "*.azure-pipelines.yaml",
        "*.azure-pipelines.yml",
        "azure-pipelines.yaml",
        "azure-pipelines.yml"
      ],
      "url": "https://raw.githubusercontent.com/microsoft/azure-pipelines-vscode/master/service-schema.json"
    }
  ],
  "licenser.author": "William Artero",
  "licenser.disableAutoHeaderInsertion": true,
  "licenser.license": "MIT",
  "liveshare.connectionMode": "relay",
  "liveshare.launcherClient": "visualStudioCode",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "npm.packageManager": "yarn",
  "projectManager.any.maxDepthRecursion": 1,
  "projectManager.git.baseFolders": [
    "$home/workspace/",
    "$home/dev/"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "vendor",
    ".terraform",
    "tmp"
  ],
  "projectManager.groupList": true,
  "projectManager.removeCurrentProjectFromList": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.useLibraryCodeForTypes": true,
  "python.languageServer": "Pylance",
  "region-viewer.markers-overrides": {
    "ruby": {
      "end": "^\\s*#?endregion\\b",
      "start": "^\\s*#?region\\b(?<name>.*)"
    }
  },
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
