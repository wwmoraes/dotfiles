  "todo-tree.general.tagGroups": {
    "FIX": [
      "FIXME",
      "FIX",
      "FIXIT",
      "HACK"
    ]
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIX",
    "FIXME",
    "FIXIT",
    "TODO",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "foreground": "#f00",
      "gutterIcon": true,
      "icon": "bug",
      "iconColour": "#f00"
    },
    "FIX": {
      "foreground": "#ff0",
      "gutterIcon": true,
      "icon": "alert",
      "iconColour": "#ff0"
    },
    "[ ]": {
      "foreground": "#ff0",
      "gutterIcon": true,
      "icon": "circle",
      "iconColour": "#ff0"
    },
    "[x]": {
      "foreground": "#0a0",
      "hideFromTree": true,
      "icon": "check-circle",
      "iconColour": "#060"
    }
  },
  "todo-tree.highlights.defaultHighlight": {
    "foreground": "#0ff",
    "gutterIcon": true,
    "icon": "comment",
    "iconColour": "#0ff",
    "type": "tag-and-subTag"
  },
  "todo-tree.regex.regex": "(--|//|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.tree.buttons.groupByTag": true,
  "todo-tree.tree.labelFormat": "${after}",
