    "dotfiles",
    "dotsecrets",
    "downlevel",
    "Eeuo",
    "entrypoint",
    "entrypoints",
    "envsubst",
    "eqeqeq",
    "Errorln",
    "ESNEXT",
    "eval",
    "fullscreen",
    "gcloud",
    "getmetatable",
    "gitaliases",
    "gitcolors",
    "gitlab",
    "GOARCH",
    "grafana",
    "hadolint",
    "Hammerspoon",
    "ifeq",
    "iife",
    "infile",
    "Infoln",
    "jsdoc",
    "keychain",
    "kube",
    "kubectl",
    "kubernetes",
    "kustomize",
    "lcov",
    "linkedin",
    "localtime",
    "maxdepth",
    "monospace",
    "moraes",
    "multiline",
    "nonblock",
    "ONBUILD",
    "operator",
    "outfile",
    "paren",
    "parens",
    "patsubst",
    "plusplus",
    "POSIX",
    "postcoverage",
    "Powerline",
    "proto",
    "protobuf",
    "pulumi",
    "runtimes",
    "setmetatable",
    "SIGINT",
    "signup",
    "SIGTERM",
    "sourcemap",
    "tini",
    "tolerations",
    "Traceln",
    "tslib",
    "typechecking",
    "typedoc",
    "Unmarshal",
    "untracked",
    "vararg",
    "venv",
    "wday",
    "wwmoraes"
