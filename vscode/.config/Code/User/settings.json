{
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "azure.resourceFilter": [
    "3a15904d-3fd9-4256-a753-beb05cdf0c6d/37523455-06d3-4fb9-8306-61a3c08f4d09"
  ],
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "breadcrumbs.enabled": true,
  "cSpell.ignorePaths": [
    "/usr/**",
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "cSpell.ignoreWords": [
    "editorconfig",
    "flexbox",
    "ismap",
    "linebreak",
    "longdesc",
    "nohoist",
    "prestart",
    "maxdepth"
  ],
  "cSpell.language": "en,fullstack",
  "cSpell.languageSettings": [
    {
      "dictionaries": [
        "Fullstack"
      ],
      "languageId": "*"
    }
  ],
  "cSpell.userWords": [
    "CLOUDSQL",
    "ESNEXT",
    "Errorln",
    "Infoln",
    "SIGINT",
    "SIGTERM",
    "Traceln",
    "artero",
    "backreference",
    "camelcase",
    "chmod",
    "codeclimate",
    "concat",
    "dockerlabelvalidate",
    "dockerlint",
    "downlevel",
    "entrypoint",
    "entrypoints",
    "eqeqeq",
    "eval",
    "gcloud",
    "gitlab",
    "iife",
    "jsdoc",
    "kube",
    "kubectl",
    "kubernetes",
    "kustomize",
    "lcov",
    "linkedin",
    "maxdepth",
    "monospace",
    "moraes",
    "multiline",
    "nonblock",
    "operator",
    "paren",
    "parens",
    "plusplus",
    "postcoverage",
    "proto",
    "protobuf",
    "pulumi",
    "runtimes",
    "signup",
    "sourcemap",
    "tolerations",
    "tslib",
    "typechecking",
    "typedoc",
    "untracked",
    "wwmoraes"
  ],
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false
  },
  "editor.cursorSurroundingLines": 5,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": "'ss03','ss07'",
  "editor.fontSize": 16,
  "editor.fontWeight": "400",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.renderFinalNewline": false,
  "editor.rulers": [
    80,
    100
  ],
  "editor.semanticHighlighting.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "explorer.confirmDelete": true,
  "files.associations": {
    "**/i3/config": "i3",
    "*.gotmpl": "go-template",
    "*.tpl": "go-template",
    "*.yml": "yaml",
    ".coveragerc": "ini",
    ".env*": "dotenv",
    ".helmignore": "gitignore",
    ".style.yapf": "ini",
    ".terraformignore": "gitignore"
  },
  "files.trimFinalNewlines": true,
  "git-graph.dialog.merge.noFastForward": false,
  "git-graph.dialog.merge.squashCommits": true,
  "git-graph.maxDepthOfRepoSearch": 2,
  "git.autoStash": true,
  "git.autofetch": true,
  "github-actions.org-features": false,
  "githubPullRequests.defaultMergeMethod": "rebase",
  "gitlens.hovers.annotations.over": "annotation",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "javascript.format.semicolons": "insert",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "licenser.author": "William Artero",
  "licenser.disableAutoHeaderInsertion": true,
  "licenser.license": "MIT",
  "npm.packageManager": "yarn",
  "projectManager.any.maxDepthRecursion": 1,
  "projectManager.git.baseFolders": [
    "$home/workspace/",
    "$home/dev/",
    "$home/../shared/dev/"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "vendor",
    ".terraform",
    "tmp"
  ],
  "projectManager.groupList": true,
  "projectManager.removeCurrentProjectFromList": true,
  "python.formatting.provider": "yapf",
  "python.languageServer": "Pylance",
  "python.showStartPage": false,
  "redhat.telemetry.enabled": true,
  "team.showWelcomeMessage": false,
  "terminal.integrated.automationShell.linux": "/usr/bin/fish",
  "terminal.integrated.automationShell.osx": "/usr/local/bin/fish",
  "terminal.integrated.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "icon": "terminal-bash",
      "path": "bash"
    },
    "fish": {
      "icon": "terminal",
      "path": "/usr/bin/fish"
    },
    "pwsh": {
      "icon": "terminal-powershell",
      "path": "pwsh"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    }
  },
  "terminal.integrated.profiles.osx": {
    "bash": {
      "icon": "terminal-bash",
      "path": "bash"
    },
    "fish": {
      "icon": "terminal",
      "path": "/usr/local/bin/fish"
    },
    "pwsh": {
      "icon": "terminal-powershell",
      "path": "pwsh"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    }
  },
  "terminal.integrated.rendererType": "dom",
  "testExplorer.useNativeTesting": true,
  "todo-tree.filtering.excludeGlobs": [
    "**/vendor/**/*.*",
    "**/node_modules/**/*.*",
    "**/.terraform/**/*.*",
    "**/tmp/**/*.*",
    "**/__*snapshots__/*.snap"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.tree.showScanModeButton": false,
  "typescript.format.semicolons": "insert",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vs-kubernetes": {
    "vs-kubernetes.knownKubeconfigs": [
      "/Users/william/.secrets/k8s/.kube/config-home",
      "/Users/william/.secrets/k8s/.kube/config-work"
    ],
    "vs-kubernetes.kubeconfig": "/Users/william/.secrets/k8s/.kube/config-work",
    "vscode-kubernetes.minikube-path.mac": "/Users/william/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "window.nativeTabs": true,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "bookmarks.lineBackground": "#157EFB22"
  },
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.startupEditor": "newUntitledFile",
  "yaml.format.enable": false,
  "testExplorer.gutterDecoration": false,
  "testExplorer.codeLens": false,
  "testExplorer.errorDecoration": false,
  "testExplorer.errorDecorationHover": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  }
}
