{
  "Lua.telemetry.enable": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "TomasHubelbauer.vscode-markdown-table-format"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "azure.resourceFilter": [
    "3a15904d-3fd9-4256-a753-beb05cdf0c6d/37523455-06d3-4fb9-8306-61a3c08f4d09"
  ],
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "breadcrumbs.enabled": true,
  "cSpell.enableFiletypes": [
    "*"
  ],
  "cSpell.ignorePaths": [
    "/usr/**",
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "cSpell.ignoreWords": [
    "editorconfig",
    "flexbox",
    "ismap",
    "linebreak",
    "longdesc",
    "nohoist",
    "prestart",
    "maxdepth"
  ],
  "cSpell.language": "en,fullstack",
  "cSpell.languageSettings": [
    {
      "dictionaries": [
        "Fullstack"
      ],
      "languageId": "*"
    }
  ],
  "cSpell.userWords": [
    "addprefix",
    "artero",
    "backreference",
    "buildx",
    "Cabuk",
    "camelcase",
    "chmod",
