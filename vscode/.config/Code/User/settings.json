{
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "terminal.integrated.automationShell.linux": "/usr/bin/fish",
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "terminal.integrated.automationShell.osx": "/usr/local/bin/fish",
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "editor.rulers": [80, 100],
  "window.zoomLevel": 0,
  "terminal.integrated.rendererType": "dom",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "editor.suggestSelection": "first",
  "window.titleBarStyle": "custom",
  "bookmarks.saveBookmarksInProject": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false
  },
  "gitlens.hovers.currentLine.over": "line",
  "workbench.editor.enablePreview": false,
  "bookmarks.useWorkaroundForFormatters": true,

  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.startupEditor": "newUntitledFile",
  "breadcrumbs.enabled": true,
  "workbench.colorCustomizations": {
    "bookmarks.lineBackground": "#157EFB22"
  },
  "terminal.integrated.fontFamily": "Fira Code",
  "git.autofetch": true,
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": true,
  "git-graph.maxDepthOfRepoSearch": 2,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": "'ss03','ss07'",
  "editor.fontWeight": "400",
  "editor.fontSize": 16,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.activityBar.visible": true,
  "editor.cursorSurroundingLines": 5,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "projectManager.any.maxDepthRecursion": 1,
  "projectManager.removeCurrentProjectFromList": true,
  "projectManager.git.baseFolders": [
    "$home/workspace/",
    "$home/dev/",
    "$home/../shared/dev/"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "vendor",
    ".terraform",
    "tmp"
  ],
  "todo-tree.filtering.excludeGlobs": [
    "**/vendor/**/*.*",
    "**/node_modules/**/*.*",
    "**/.terraform/**/*.*",
    "**/tmp/**/*.*",
    "**/__*snapshots__/*.snap"
  ],
  "cSpell.language": "en,fullstack",
  "cSpell.languageSettings": [
    { "languageId": "*", "dictionaries": ["Fullstack"] }
  ],
  "cSpell.userWords": [
    "CLOUDSQL",
    "ESNEXT",
    "Errorln",
    "Infoln",
    "MESSAGEBIRD",
    "SIGINT",
    "SIGTERM",
    "Traceln",
    "artero",
    "backreference",
    "camelcase",
    "chmod",
    "codeclimate",
    "concat",
    "dockerlabelvalidate",
    "dockerlint",
    "downlevel",
    "entrypoint",
    "entrypoints",
    "eqeqeq",
    "eval",
    "gcloud",
    "gitlab",
    "iife",
    "jsdoc",
    "kubectl",
    "kubernetes",
    "kustomize",
    "lcov",
    "linkedin",
    "maxdepth",
    "moraes",
    "multiline",
    "nonblock",
    "operator",
    "paren",
    "parens",
    "plusplus",
    "postcoverage",
    "proto",
    "protobuf",
    "runtimes",
    "signup",
    "sourcemap",
    "tolerations",
    "tslib",
    "typechecking",
    "typedoc",
    "untracked",
    "wwmoraes"
  ],
  "cSpell.ignoreWords": [
    "editorconfig",
    "flexbox",
    "ismap",
    "linebreak",
    "longdesc",
    "nohoist",
    "prestart",
    "maxdepth"
  ],
  "cSpell.ignorePaths": [
    "/usr/**",
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "vscode-icons",
  "files.associations": {
    "**/i3/config": "i3",
    "Dockerfile_*": "dockerfile",
    ".env*": "dotenv",
    ".terraformignore": "gitignore",
    "*.tpl": "go-template",
    ".helmignore": "gitignore"
  },
  "git-graph.dialog.merge.noFastForward": false,
  "git-graph.dialog.merge.squashCommits": true,
  "licenser.author": "William Artero",
  "projectManager.groupList": true,
  "window.nativeTabs": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.trimFinalNewlines": true,
  "editor.renderFinalNewline": false,
  "todo-tree.tree.showScanModeButton": false,
  "git.autoStash": true,
  "npm.packageManager": "yarn",
  "workbench.sideBar.location": "left",
  "licenser.license": "MIT",
  "githubPullRequests.defaultMergeMethod": "rebase",
  "javascript.format.semicolons": "insert",
  "typescript.format.semicolons": "insert",
  "python.languageServer": "Microsoft",
  "go.useLanguageServer": true,
  "editor.formatOnSave": true,
  "github-actions.org-features": false
}
