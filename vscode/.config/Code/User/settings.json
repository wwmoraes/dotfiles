{
  "terminal.integrated.shell.linux": "fish",
  "terminal.integrated.automationShell.linux": "fish",
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80,
    100
  ],
  "window.zoomLevel": 0,
  "terminal.integrated.rendererType": "dom",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "editor.suggestSelection": "first",
  "window.titleBarStyle": "custom",
  "bookmarks.saveBookmarksInProject": true,
  "types-autoinstaller.saveAsDevDependency": true,
  "eslint.autoFixOnSave": true,
  "gitlens.hovers.currentLine.over": "line",
  "workbench.editor.enablePreview": false,
  "bookmarks.useWorkaroundForFormatters": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.startupEditor": "newUntitledFile",
  "breadcrumbs.enabled": true,
  "bookmarks.backgroundLineColor": "rgba(21, 126, 251, 0.1)",
  "go.useLanguageServer": true,
  "phpcs.standard": "PSR2",
  "go.alternateTools": {
    "go": "~/.go/bin/go"
  },
  "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
  "git.autofetch": true,
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": true,
  "git-graph.maxDepthOfRepoSearch": 2,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.fontSize": 16,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.activityBar.visible": true,
  "editor.cursorSurroundingLines": 5,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "projectManager.any.maxDepthRecursion": 1,
  "projectManager.removeCurrentProjectFromList": true,
  "projectManager.git.baseFolders": [
    "$home/.dotfiles",
    "$home/.dotsecrets",
    "$home/workspace/",
    "$home/dev/",
    "$home/../shared/dev/"
  ],
  "projectManager.vscode.baseFolders": [
    "$home/workspace/",
    "$home/dev/",
    "$home/../shared/dev/"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "vendor"
  ],
  "projectManager.vscode.ignoredFolders": [
    "node_modules",
    "vendor"
  ],
  "cSpell.userWords": [
    "nohoist",
    "wwmoraes"
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
