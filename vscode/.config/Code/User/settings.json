{
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "terminal.integrated.automationShell.linux": "/usr/bin/fish",
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80,
    100
  ],
  "window.zoomLevel": 0,
  "terminal.integrated.rendererType": "dom",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "editor.suggestSelection": "first",
  "window.titleBarStyle": "custom",
  "bookmarks.saveBookmarksInProject": true,
  "types-autoinstaller.saveAsDevDependency": true,
  "eslint.autoFixOnSave": true,
  "gitlens.hovers.currentLine.over": "line",
  "workbench.editor.enablePreview": false,
  "bookmarks.useWorkaroundForFormatters": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.startupEditor": "newUntitledFile",
  "breadcrumbs.enabled": true,
  "bookmarks.backgroundLineColor": "rgba(21, 126, 251, 0.1)",
  "go.useLanguageServer": true,
  "go.alternateTools": {
    "go": "~/.go/bin/go"
  },
  "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
  "git.autofetch": true,
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": false,
  "git-graph.maxDepthOfRepoSearch": 2,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.fontSize": 16,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.activityBar.visible": true,
  "editor.cursorSurroundingLines": 5,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "projectManager.any.maxDepthRecursion": 1,
  "projectManager.removeCurrentProjectFromList": true,
  "projectManager.git.baseFolders": [
    "$home/.dotfiles",
    "$home/.dotsecrets",
    "$home/workspace/",
    "$home/dev/",
    "$home/../shared/dev/"
  ],
  "projectManager.vscode.baseFolders": [
    "$home/workspace/",
    "$home/dev/",
    "$home/../shared/dev/"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "vendor"
  ],
  "projectManager.vscode.ignoredFolders": [
    "node_modules",
    "vendor"
  ],
  "todo-tree.filtering.excludeGlobs": [
    "**/vendor/**/*.*",
    "**/node_modules/**/*.*",
  ],
  "cSpell.userWords": [
    "nohoist",
    "wwmoraes"
  ],
  "cSpell.ignorePaths": [
    "/usr/**",
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "vscode-icons",
  "files.associations": {
    "**/i3/config": "i3",
    "Dockerfile_*": "dockerfile",
    ".env*": "dotenv",
    ".terraformignore": "gitignore"
  },
  "vs-kubernetes": {
    "vs-kubernetes.helm-path": "/usr/local/bin/helm",
    "vs-kubernetes.kubectl-path": "/usr/local/bin/kubectl",
    "vs-kubernetes.draft-path": "/usr/local/bin/draft",
    "vs-kubernetes.minikube-path": "/usr/local/bin/minikube"
  },
  "git-graph.dialog.merge.noFastForward": false,
  "git-graph.dialog.merge.squashCommits": true,
  "git-graph.fetchAndPrune": true,
  "licenser.author": "William Wagner Moraes Artero",
  "licenser.disableAutoHeaderInsertion": true,
  "projectManager.groupList": true
}
