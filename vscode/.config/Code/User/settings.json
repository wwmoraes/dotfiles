  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false
  },
  "editor.cursorSurroundingLines": 5,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": "'ss01', 'ss03', 'ss05', 'ss07', 'ss09', 'cv12', 'cv21', 'cv25', 'cv26', 'cv28', 'cv29', 'cv30'",
  "editor.fontSize": 16,
  "editor.fontWeight": "400",
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.renderFinalNewline": false,
  "editor.rulers": [
    80,
    100
  ],
  "editor.semanticHighlighting.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "explorer.confirmDelete": true,
  "files.associations": {
    "**/.ssh/config": "ssh_config",
    "**/.ssh/config.d/*/*": "ssh_config",
    "**/i3/config": "i3",
    "*.gotmpl": "go-template",
    "*.gtpl": "go-template",
    "*.tfstate": "json",
    "*.tpl": "go-template",
    "*.yml": "yaml",
    ".coveragerc": "ini",
    ".env*": "dotenv",
    ".gitconfig": "properties",
    ".gitignore-global": "ignore",
    ".helmignore": "gitignore",
    ".style.yapf": "ini",
    ".terraformignore": "gitignore"
  },
  "files.trimFinalNewlines": true,
  "git-graph.dialog.merge.noFastForward": false,
  "git-graph.dialog.merge.squashCommits": true,
  "git-graph.maxDepthOfRepoSearch": 2,
  "git.autoStash": true,
  "git.autofetch": true,
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.statusBar.enabled": false,
  "go.buildOnSave": "off",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "gremlins.characters": {
    "2013": {
      "description": "en dash",
      "level": "warning"
    },
    "2018": {
      "description": "left single quotation mark",
      "level": "warning"
    },
    "2019": {
      "description": "right single quotation mark",
      "level": "warning"
    },
    "2029": {
      "description": "paragraph separator",
      "level": "error",
      "zeroWidth": true
    },
    "0003": {
      "description": "end of text",
      "level": "warning"
    },
    "000b": {
      "description": "line tabulation",
      "level": "warning"
    },
    "00a0": {
      "description": "non breaking space",
      "level": "info"
    },
    "00ad": {
      "description": "soft hyphen",
      "level": "info"
    },
    "200b": {
      "description": "zero width space",
      "level": "error",
      "zeroWidth": true
    },
    "200c": {
      "description": "zero width non-joiner",
      "level": "warning",
      "zeroWidth": true
    },
    "200e": {
      "description": "left-to-right mark",
      "level": "error",
      "zeroWidth": true
    },
    "201c": {
      "description": "left double quotation mark",
      "level": "warning"
    },
    "201d": {
      "description": "right double quotation mark",
      "level": "warning"
    },
    "202c": {
      "description": "pop directional formatting",
      "level": "error",
      "zeroWidth": true
    },
    "202d": {
      "description": "left-to-right override",
      "level": "error",
      "zeroWidth": true
    },
    "202e": {
      "description": "right-to-left override",
      "level": "error",
      "zeroWidth": true
    },
    "fffc": {
      "description": "object replacement character",
      "level": "error",
      "zeroWidth": true
    }
  },
  "haskell.manageHLS": "GHCup",
  "haskell.toolchain": {
    "cabal": "recommended",
    "hls": "recommended",
    "stack": null
  },
  "haskell.upgradeGHCup": true,
  "javascript.format.semicolons": "insert",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": true,
  "json.schemas": [
    {
      "fileMatch": [
        "/package.json"
      ],
      "url": "https://json.schemastore.org/package.json"
    }
  ],
  "licenser.author": "William Artero",
  "licenser.disableAutoHeaderInsertion": true,
  "licenser.license": "MIT",
  "liveshare.presence": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "npm.packageManager": "yarn",
  "projectManager.any.maxDepthRecursion": 1,
  "projectManager.git.baseFolders": [
    "$home/workspace/",
    "$home/dev/"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "vendor",
    ".terraform",
    "tmp"
  ],
  "projectManager.groupList": true,
  "projectManager.removeCurrentProjectFromList": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.useLibraryCodeForTypes": true,
  "python.formatting.provider": "yapf",
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": true,
  "shellcheck.customArgs": [
    "-x"
  ],
  "terminal.integrated.automationProfile.linux": {
    "icon": "terminal",
    "path": "/usr/bin/fish"
  },
  "terminal.integrated.automationProfile.osx": {
    "icon": "terminal",
    "path": "/usr/local/bin/fish"
  },
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "icon": "terminal-bash",
      "path": "bash"
    },
    "fish": {
      "icon": "terminal",
      "path": "/usr/bin/fish"
    },
    "pwsh": {
      "icon": "terminal-powershell",
      "path": "pwsh"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    }
  },
  "terminal.integrated.profiles.osx": {
    "bash": {
      "icon": "terminal-bash",
      "path": "bash"
    },
    "fish": {
      "icon": "terminal",
      "path": "/usr/local/bin/fish"
    },
    "pwsh": {
      "icon": "terminal-powershell",
      "path": "pwsh"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    }
  },
  "terminal.integrated.shellIntegration.enabled": false,
  "testExplorer.codeLens": false,
  "testExplorer.errorDecoration": false,
  "testExplorer.errorDecorationHover": false,
  "testExplorer.gutterDecoration": false,
  "testExplorer.useNativeTesting": true,
  "todo-tree.filtering.excludeGlobs": [
    "**/vendor/**/*.*",
    "**/node_modules/**/*.*",
    "**/.terraform/**/*.*",
    "**/tmp/**/*.*",
    "**/__*snapshots__/*.snap"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "typescript.format.semicolons": "insert",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsicons.dontShowNewVersionMessage": true,
  "window.nativeTabs": true,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "bookmarks.lineBackground": "#157EFB22"
  },
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.startupEditor": "none",
  "workspace.alwaysShowToggle": true,
  "write-good.languages": [
    "markdown",
    "plaintext"
  ],
  "write-good.only-lint-on-save": false,
  "write-good.write-good-config": {
    "adverb": true,
    "cliches": true,
    "eprime": false,
    "illusion": true,
    "passive": true,
    "so": true,
    "thereIs": true,
    "tooWordy": true,
    "weasel": true
  },
  "yaml.format.enable": false,
  "yaml.schemaStore.enable": true,
  "yaml.schemas": {
    "https://json.schemastore.org/chart.json": "Chart.yaml",
    "https://json.schemastore.org/container-structure-test.json": "structure-test.yaml",
    "https://json.schemastore.org/github-workflow.json": "**/.github/workflows/*",
    "https://json.schemastore.org/kustomization.json": "kustomization.yaml",
    "https://raw.githubusercontent.com/microsoft/azure-pipelines-vscode/master/service-schema.json": "azure-pipelines.yml",
    "https://yarnpkg.com/configuration/yarnrc.json": ".yarnrc.yml"
  }
}